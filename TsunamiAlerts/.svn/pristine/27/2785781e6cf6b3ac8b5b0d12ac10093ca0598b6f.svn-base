package com.crmindz.model;

/*
 * stores the alerts related to Indonesia
 */
public class IndonesiaAlerts {
	private String alert = "Indonesia - tsunami advisory: people at coastal areas should be careful";

	/**
	 * @return the alert
	 */
	public String getAlert() {
		return alert;
	}

	/**
	 * @param alert
	 *            the alert to set
	 */
	public void setAlert(String alert) {
		this.alert = alert;
	}
}

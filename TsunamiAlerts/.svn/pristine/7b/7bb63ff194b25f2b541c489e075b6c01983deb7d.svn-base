package com.crmindz.model;

import java.util.Date;

/*
 * stores the alerts related to India
 */
public class IndiaAlerts {
	private String alert = "India - tsunami watch: earthquake observed at" + new Date() + " possibility of tsunami occurance";

	/**
	 * @return the alert
	 */
	public String getAlert() {
		return alert;
	}

	/**
	 * @param alert
	 *            the alert to set
	 */
	public void setAlert(String alert) {
		this.alert = alert;
	}

}

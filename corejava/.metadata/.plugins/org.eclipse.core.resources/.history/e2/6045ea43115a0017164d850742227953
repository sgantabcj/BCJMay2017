package com.crmindz.bcjmay2017.datatypes.arrays;

public class JaggedArray {
	public static void main(String[] args) {
		String[][] jaggedArray = {
				{"Student","NameT","Tutorial 1","Tutorial 2","Tutorial 3"},
				{"Tony","JAVA","C","C++"},
				{"Thomas","JAVA","UNIX"},
				{"Dinil","Linux","Oracle"},
				{"Delvin","RDBMS","C#","ORACLE"}
		};
	}
}
Tony 			JAVA 		C 		C++
Thomas 		JAVA 		UNIX
Dinil 			Linux 		Oracle
Delvin 			RDBMS 		C# 		ORACLE

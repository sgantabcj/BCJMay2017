package com.crmindz.bcjmay2017.introduction.lab1;

/*
 * Prints desired shape
 */

public class Shape {
	public static void main(String[] args) {
		printShape();
	}
//	method to print desired hexagon shape
	private static void printShape() {
		// TODO Auto-generated method stub
		System.out.println("  _______");
		System.out.println(" /       \\");
		System.out.println("/         \\");
		System.out.println("-\"-\'-\"-\'-\"-");
		System.out.println("\\         /");
		System.out.println(" \\_______/");
	}
}

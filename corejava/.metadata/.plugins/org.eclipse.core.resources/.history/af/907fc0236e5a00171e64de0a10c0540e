package com.crmindz.bcjmay2017.introduction.lab3;

public class FightSong {
	public static void main(String[] args) {
		printGoTeam();
		printLn();
		printGoTeam();
		printBestInWest();
		printGoTeam();
		printLn();
		printGoTeam();
		printBestInWest();
		printGoTeam();
		printLn();
		printGoTeam();
	}
	static void printGoTeam(){
		System.out.println("Go, team, go!\nYou can do it.");
	}
	static void printBestInWest(){
		System.out.println("You're the best,\nIn the West.");
	}
	static void printLn(){
		System.out.println();
	}
	
}
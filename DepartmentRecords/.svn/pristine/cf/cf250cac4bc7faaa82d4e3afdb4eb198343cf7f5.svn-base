package com.crmindz.departmentrecords.dao;

import java.util.List;

import com.crmindz.departmentrecords.data.DepartmentData;

public class DepartmentDao {
	DepartmentData deptData = new DepartmentData();
	
	/**
	 * @return the departmentRecords from DepartmentData class
	 */
	public List<String> getDepartmentData() {
		return deptData.getDepartmentData();
	}

	/**
	 * @param departmentRecords passes to the DepartmentData class
	 */
	public void saveDepartmentData(List<String> departmentRecords) {
		deptData.setDepartmentData(departmentRecords);
	}
}
